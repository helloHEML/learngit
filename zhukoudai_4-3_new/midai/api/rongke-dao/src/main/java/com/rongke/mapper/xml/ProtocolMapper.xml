<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.ProtocolMapper">    <resultMap type="com.rongke.model.Protocol" id="rmProtocol">        <id property="id" column="id" />        <result property="type" column="type"/>        <result property="status" column="status"/>        <result property="content" column="content"/>        <result property="picUrl" column="picUrl"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>    </resultMap>    <resultMap type="com.rongke.model.Protocol" id="mProtocol">        <id property="id" column="protocol_id" />        <result property="type" column="protocol_type"/>        <result property="status" column="protocol_status"/>        <result property="content" column="protocol_content"/>        <result property="picUrl" column="protocol_picUrl"/>        <result property="gmtDatetime" column="protocol_gmtDatetime"/>        <result property="uptDatetime" column="protocol_uptDatetime"/>    </resultMap>    <sql id="mProtocolPkSql">        protocol.id AS protocol_id,        protocol.type AS protocol_type,        protocol.status AS protocol_status,        protocol.content AS protocol_content,        protocol.pic_url AS protocol_picUrl,        protocol.gmt_datetime AS protocol_gmtDatetime,        protocol.upt_datetime AS protocol_uptDatetime    </sql>    <sql id="mProtocolSql">        id AS id,        type AS type,        status AS status,        content AS content,        pic_url AS picUrl,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="selectByCondition" parameterType="Protocol" resultMap="rmProtocol">    select    <include refid="mProtocolSql"></include>    from protocol    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="type!= null">            AND type=#{type}        </if>        <if test="status!= null">            AND status=#{status}        </if>        <if test="content!= null">            AND content=#{content}        </if>        <if test="picUrl!= null">            AND pic_url=#{picUrl}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>    </where>    </select>    </mapper>