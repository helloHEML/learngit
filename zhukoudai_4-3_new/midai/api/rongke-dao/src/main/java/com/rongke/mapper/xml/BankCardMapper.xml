<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.BankCardMapper">    <resultMap type="com.rongke.model.BankCard" id="rmBankCard">        <id property="id" column="id" />        <result property="userId" column="userId"/>        <result property="idCardNo" column="idCardNo"/>        <result property="bankCardNo" column="bankCardNo"/>        <result property="status" column="status"/>        <result property="phone" column="phone"/>        <result property="bankCardName" column="bankCardName"/>    </resultMap>    <resultMap type="com.rongke.model.BankCard" id="mBankCard">        <id property="id" column="bankCard_id" />        <result property="userId" column="bankCard_userId"/>        <result property="idCardNo" column="bankCard_idCardNo"/>        <result property="bankCardNo" column="bankCard_bankCardNo"/>        <result property="status" column="bankCard_status"/>        <result property="phone" column="bankCard_phone"/>        <result property="name" column="bankCard_name"/>        <result property="bankCardName" column="bankCard_bankCardName"/>        <result property="gmtDatetime" column="bankCard_gmtDatetime"/>    </resultMap>    <sql id="mBankCardPkSql">        bank_card.id AS bankCard_id,        bank_card.user_id AS bankCard_userId,        bank_card.id_card_no AS bankCard_idCardNo,        bank_card.bank_card_no AS bankCard_bankCardNo,        bank_card.status AS bankCard_status,        bank_card.phone AS bankCard_phone,        bank_card.name AS bankCard_name,        bank_card.bank_card_name AS bankCard_bankCardName,        bank_card.gmt_datetime AS bankCard_gmtDatetime    </sql>    <sql id="mBankCardSql">        id AS id,        user_id AS userId,        id_card_no AS idCardNo,        bank_card_no AS bankCardNo,        status AS status,        phone AS phone,        bank_card_name AS bankCardName    </sql>     <select id="selectByPage" parameterType="java.lang.String" resultMap="mBankCard">         SELECT         <include refid="mBankCardPkSql"></include>         FROM bank_card , authentication         WHERE bank_card.user_id=authentication.user_id         <if test="adminId!=null and adminId!=''">          and   authentication.auditor_id=#{adminId}         </if>         <if test="name!=null and name!=''">             and  bank_card.name=#{name}         </if>         <if test="status!=null and status!=''">             and  bank_card.status=#{status}         </if>         ORDER BY bank_card.gmt_datetime DESC         <if test="_parameter.containsKey('startIndex')">             limit #{startIndex},#{size}         </if>     </select>      <select id="selectCount" parameterType="java.lang.String" resultType="java.lang.Integer">        SELECT        COUNT(1)         FROM         bank_card , authentication          WHERE bank_card.user_id=authentication.user_id          <if test="adminId!=null and adminId!=''">              and  authentication.auditor_id=#{adminId}          </if>          <if test="name!=null and name!=''">              and  bank_card.name=#{name}          </if>          <if test="status!=null and status!=''">              and  bank_card.status=#{status}          </if>      </select>    </mapper>