<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.HelpCenterMapper">    <resultMap type="com.rongke.model.HelpCenter" id="rmHelpCenter">        <id property="id" column="id" />        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>        <result property="content" column="content"/>        <result property="title" column="title"/>        <result property="type" column="type"/>    </resultMap>    <resultMap type="com.rongke.model.HelpCenter" id="mHelpCenter">        <id property="id" column="helpCenter_id" />        <result property="gmtDatetime" column="helpCenter_gmtDatetime"/>        <result property="uptDatetime" column="helpCenter_uptDatetime"/>        <result property="content" column="helpCenter_content"/>        <result property="title" column="helpCenter_title"/>        <result property="type" column="helpCenter_type"/>    </resultMap>    <sql id="mHelpCenterPkSql">        help_center.id AS helpCenter_id,        help_center.gmt_datetime AS helpCenter_gmtDatetime,        help_center.upt_datetime AS helpCenter_uptDatetime,        help_center.content AS helpCenter_content,        help_center.title AS helpCenter_title,        help_center.type AS helpCenter_type    </sql>    <sql id="mHelpCenterSql">        id AS id,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime,        content AS content,        title AS title,        type AS type    </sql>    <select id="selectByCondition" parameterType="HelpCenter" resultMap="rmHelpCenter">    select    <include refid="mHelpCenterSql"></include>    from help_center    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>        <if test="content!= null">            AND content=#{content}        </if>        <if test="title!= null">            AND title=#{title}        </if>        <if test="type!= null">            AND type=#{type}        </if>    </where>    </select>    </mapper>