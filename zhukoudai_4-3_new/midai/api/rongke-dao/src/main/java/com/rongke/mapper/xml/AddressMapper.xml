<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.AddressMapper">    <resultMap type="com.rongke.model.Address" id="rmAddress">        <id property="id" column="id" />        <result property="province" column="province"/>        <result property="city" column="city"/>        <result property="area" column="area"/>        <result property="addressDetail" column="addressDetail"/>        <result property="status" column="status"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>    </resultMap>    <resultMap type="com.rongke.model.Address" id="mAddress">        <id property="id" column="address_id" />        <result property="province" column="address_province"/>        <result property="city" column="address_city"/>        <result property="area" column="address_area"/>        <result property="addressDetail" column="address_addressDetail"/>        <result property="status" column="address_status"/>        <result property="gmtDatetime" column="address_gmtDatetime"/>        <result property="uptDatetime" column="address_uptDatetime"/>    </resultMap>    <sql id="mAddressPkSql">        address.id AS address_id,        address.province AS address_province,        address.city AS address_city,        address.area AS address_area,        address.address_detail AS address_addressDetail,        address.status AS address_status,        address.gmt_datetime AS address_gmtDatetime,        address.upt_datetime AS address_uptDatetime    </sql>    <sql id="mAddressSql">        id AS id,        province AS province,        city AS city,        area AS area,        address_detail AS addressDetail,        status AS status,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="selectByCondition" parameterType="Address" resultMap="rmAddress">    select    <include refid="mAddressSql"></include>    from address    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="province!= null">            AND province=#{province}        </if>        <if test="city!= null">            AND city=#{city}        </if>        <if test="area!= null">            AND area=#{area}        </if>        <if test="addressDetail!= null">            AND address_detail=#{addressDetail}        </if>        <if test="status!= null">            AND status=#{status}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>    </where>    </select>    </mapper>