<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.FeedbackMapper">    <resultMap type="com.rongke.model.Feedback" id="rmFeedback">        <id property="id" column="id" />        <result property="userId" column="userId"/>        <result property="content" column="content"/>        <result property="phone" column="phone"/>        <result property="imgUrl" column="imgUrl"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>        <association property="user" javaType="com.rongke.model.User" >            <id property="id" column="user_id" />        </association>    </resultMap>    <resultMap type="com.rongke.model.Feedback" id="mFeedback">        <id property="id" column="feedback_id" />        <result property="userId" column="feedback_userId"/>        <result property="content" column="feedback_content"/>        <result property="phone" column="feedback_phone"/>        <result property="imgUrl" column="feedback_imgUrl"/>        <result property="gmtDatetime" column="feedback_gmtDatetime"/>        <result property="uptDatetime" column="feedback_uptDatetime"/>        <association property="user" javaType="com.rongke.model.User" >            <id property="id" column="user_id" />        </association>    </resultMap>    <sql id="mFeedbackPkSql">        feedback.id AS feedback_id,        feedback.user_id AS feedback_userId,        feedback.content AS feedback_content,        feedback.phone AS feedback_phone,        feedback.img_url AS feedback_imgUrl,        feedback.gmt_datetime AS feedback_gmtDatetime,        feedback.upt_datetime AS feedback_uptDatetime    </sql>    <sql id="mFeedbackSql">        id AS id,        user_id AS userId,        content AS content,        phone AS phone,        img_url AS imgUrl,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="selectByCondition" parameterType="Feedback" resultMap="rmFeedback">    select    <include refid="mFeedbackSql"></include>    from feedback    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="userId!= null">            AND user_id=#{userId}        </if>        <if test="content!= null">            AND content=#{content}        </if>        <if test="phone!= null">            AND phone=#{phone}        </if>        <if test="imgUrl!= null">            AND img_url=#{imgUrl}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>    </where>    </select>    </mapper>