<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.DictionaryDataMapper">    <resultMap type="com.rongke.model.DictionaryData" id="rmDictionaryData">        <id property="id" column="id" />        <result property="dictName" column="dictName"/>        <result property="dictdataName" column="dictdataName"/>        <result property="dictdataValue" column="dictdataValue"/>        <result property="status" column="status"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>    </resultMap>    <resultMap type="com.rongke.model.DictionaryData" id="mDictionaryData">        <id property="id" column="dictionaryData_id" />        <result property="dictName" column="dictionaryData_dictName"/>        <result property="dictdataName" column="dictionaryData_dictdataName"/>        <result property="dictdataValue" column="dictionaryData_dictdataValue"/>        <result property="status" column="dictionaryData_status"/>        <result property="gmtDatetime" column="dictionaryData_gmtDatetime"/>        <result property="uptDatetime" column="dictionaryData_uptDatetime"/>    </resultMap>    <sql id="mDictionaryDataPkSql">        dictionary_data.id AS dictionaryData_id,        dictionary_data.dict_name AS dictionaryData_dictName,        dictionary_data.dictdata_name AS dictionaryData_dictdataName,        dictionary_data.dictdata_value AS dictionaryData_dictdataValue,        dictionary_data.status AS dictionaryData_status,        dictionary_data.gmt_datetime AS dictionaryData_gmtDatetime,        dictionary_data.upt_datetime AS dictionaryData_uptDatetime    </sql>    <sql id="mDictionaryDataSql">        id AS id,        dict_name AS dictName,        dictdata_name AS dictdataName,        dictdata_value AS dictdataValue,        status AS status,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="selectByCondition" parameterType="DictionaryData" resultMap="rmDictionaryData">    select    <include refid="mDictionaryDataSql"></include>    from dictionary_data    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="dictName!= null">            AND dict_name=#{dictName}        </if>        <if test="dictdataName!= null">            AND dictdata_name=#{dictdataName}        </if>        <if test="dictdataValue!= null">            AND dictdata_value=#{dictdataValue}        </if>        <if test="status!= null">            AND status=#{status}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>    </where>    </select>    <update id="updateListByDictdataName" parameterType="java.util.List">        UPDATE dictionary_data        SET dictdata_value = CASE dictdata_name        <foreach close="" collection="list" index="index" item="item" open="" separator="">            WHEN #{item.dictdataName} THEN #{item.dictdataValue}        </foreach>        END        WHERE dictdata_name IN        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">            #{item.dictdataName}        </foreach>    </update>    <select id="findOneByDictdataName" parameterType="java.lang.String" resultType="java.lang.String">        select dictdata_value FROM dictionary_data WHERE dictdata_name=#{dictdataName}    </select>    </mapper>