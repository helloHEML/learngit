<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.DictionaryMapper">    <resultMap type="com.rongke.model.Dictionary" id="rmDictionary">        <id property="id" column="id" />        <result property="dictName" column="dictName"/>    </resultMap>    <resultMap type="com.rongke.model.Dictionary" id="mDictionary">        <id property="id" column="dictionary_id" />        <result property="dictName" column="dictionary_dictName"/>    </resultMap>    <sql id="mDictionaryPkSql">        dictionary.id AS dictionary_id,        dictionary.dict_name AS dictionary_dictName    </sql>    <sql id="mDictionarySql">        id AS id,        dict_name AS dictName    </sql>    <select id="selectByCondition" parameterType="Dictionary" resultMap="rmDictionary">    select    <include refid="mDictionarySql"></include>    from dictionary    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="dictName!= null">            AND dict_name=#{dictName}        </if>    </where>    </select>    </mapper>