<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.VersionMapper">    <resultMap type="com.rongke.model.Version" id="rmVersion">        <id property="id" column="id" />        <result property="code" column="code"/>        <result property="link" column="link"/>        <result property="status" column="status"/>        <result property="type" column="type"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>    </resultMap>    <resultMap type="com.rongke.model.Version" id="mVersion">        <id property="id" column="version_id" />        <result property="code" column="version_code"/>        <result property="link" column="version_link"/>        <result property="status" column="version_status"/>        <result property="type" column="version_type"/>        <result property="gmtDatetime" column="version_gmtDatetime"/>        <result property="uptDatetime" column="version_uptDatetime"/>    </resultMap>    <sql id="mVersionPkSql">        version.id AS version_id,        version.code AS version_code,        version.link AS version_link,        version.status AS version_status,        version.type AS version_type,        version.gmt_datetime AS version_gmtDatetime,        version.upt_datetime AS version_uptDatetime    </sql>    <sql id="mVersionSql">        id AS id,        code AS code,        link AS link,        status AS status,        type AS  type,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="versionList"  resultType="java.util.Map">        SELECT v.id,v.code,v.type,v.force,v.status,v.gmt_datetime        FROM version v        <where>            ${ew.sqlSegment}        </where>        ORDER BY v.gmt_datetime DESC    </select>    </mapper>