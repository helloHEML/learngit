<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.CallRecordsMapper">    <resultMap type="com.rongke.model.CallRecords" id="mCallRecords" autoMapping="true">        <association property="user_id" javaType="com.rongke.model.User" autoMapping="true" />    </resultMap>    <sql id="mColumnsSql">        id AS id,        user_id AS userId,        phone AS phone,        call_time_z AS callTimeZ,        call_count_z AS callCountZ,        call_time_b AS callTimeB,        call_count_b AS callCountB,        status AS status    </sql>    <insert id="saveCallRecords" parameterType="java.util.List">        insert into call_records (user_id,phone,call_time_z,call_count_z,call_time_b,call_count_b,user_name,link,p_user_id) values        <foreach collection ="list" item="b" index= "index" separator =",">            (            #{b.userId},            #{b.phone},            #{b.callTimeZ},            #{b.callCountZ},            #{b.callTimeB},            #{b.callCountB},            #{b.userName},            #{b.link},            #{b.pUserId}            )        </foreach>    </insert></mapper>