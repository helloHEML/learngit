<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.AdminPermissionMapper">    <resultMap type="com.rongke.model.AdminPermission" id="rmAdminPermission">        <id property="id" column="id" />        <result property="name" column="name"/>        <result property="type" column="type"/>        <result property="url" column="url"/>        <result property="percode" column="percode"/>        <result property="parentid" column="parentid"/>        <result property="available" column="available"/>    </resultMap>    <resultMap type="com.rongke.model.AdminPermission" id="mAdminPermission">        <id property="id" column="adminPermission_id" />        <result property="name" column="adminPermission_name"/>        <result property="type" column="adminPermission_type"/>        <result property="url" column="adminPermission_url"/>        <result property="percode" column="adminPermission_percode"/>        <result property="parentid" column="adminPermission_parentid"/>        <result property="available" column="adminPermission_available"/>    </resultMap>    <sql id="mAdminPermissionPkSql">        admin_permission.id AS adminPermission_id,        admin_permission.name AS adminPermission_name,        admin_permission.type AS adminPermission_type,        admin_permission.url AS adminPermission_url,        admin_permission.percode AS adminPermission_percode,        admin_permission.parentid AS adminPermission_parentid,        admin_permission.available AS adminPermission_available    </sql>    <sql id="mAdminPermissionSql">        id AS id,        name AS name,        type AS type,        url AS url,        percode AS percode,        parentid AS parentid,        available AS available    </sql>     <select id="selectMenus" parameterType="java.lang.Integer" resultType="java.lang.Integer">         SELECT           admin_permission_id         FROM         admin_permission,admin_role_permission         WHERE         admin_permission.id=admin_role_permission.admin_permission_id         AND         admin_role_permission.admin_role_id=#{roleId}     </select>    </mapper>