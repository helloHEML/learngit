<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.EachPictureMapper">    <resultMap type="com.rongke.model.EachPicture" id="rmEachPicture">        <id property="id" column="id" />        <result property="imgUrl" column="imgUrl"/>        <result property="linkUrl" column="linkUrl"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>    </resultMap>    <resultMap type="com.rongke.model.EachPicture" id="mEachPicture">        <id property="id" column="EachPicture_id" />        <result property="imgUrl" column="eachPicture_imgUrl"/>        <result property="linkUrl" column="eachPicture_linkUrl"/>        <result property="gmtDatetime" column="eachPicture_gmtDatetime"/>        <result property="uptDatetime" column="eachPicture_uptDatetime"/>    </resultMap>    <sql id="mEachPicturePkSql">        each_picture.id AS eachPicture_id,        each_picture.img_url AS eachPicture_imgUrl,        each_picture.link_url AS eachPicture_linkUrl,        each_picture.gmt_datetime AS eachPicture_gmtDatetime,        each_picture.upt_datetime AS eachPicture_uptDatetime    </sql>    <sql id="mEachPictureSql">        id AS id,        img_url AS imgUrl,        link_url AS linkUrl,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="selectByCondition" parameterType="EachPicture" resultMap="rmEachPicture">    select    <include refid="mEachPictureSql"></include>    from each_picture    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="imgUrl!= null">            AND img_url=#{imgUrl}        </if>        <if test="linkUrl!= null">            AND link_url=#{linkUrl}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>    </where>    </select>    </mapper>