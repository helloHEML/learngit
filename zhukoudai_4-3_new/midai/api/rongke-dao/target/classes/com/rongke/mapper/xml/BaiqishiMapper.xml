<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.BaiqishiMapper">    <resultMap type="com.rongke.model.Baiqishi" id="rmBaiqishi">        <id property="id" column="id" />        <result property="userId" column="userId"/>        <result property="resultCode" column="resultCode"/>        <result property="resultDesc" column="resultDesc"/>        <result property="strategySet" column="strategySet"/>        <result property="flowNo" column="flowNo"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>    </resultMap>    <resultMap type="com.rongke.model.Baiqishi" id="mBaiqishi">        <id property="id" column="baiqishi_id" />        <result property="userId" column="baiqishi_userId"/>        <result property="resultCode" column="baiqishi_resultCode"/>        <result property="resultDesc" column="baiqishi_resultDesc"/>        <result property="strategySet" column="baiqishi_strategySet"/>        <result property="flowNo" column="baiqishi_flowNo"/>        <result property="gmtDatetime" column="baiqishi_gmtDatetime"/>        <result property="uptDatetime" column="baiqishi_uptDatetime"/>    </resultMap>    <sql id="mBaiqishiPkSql">        baiqishi.id AS baiqishi_id,        baiqishi.user_id AS baiqishi_userId,        baiqishi.result_code AS baiqishi_resultCode,        baiqishi.result_desc AS baiqishi_resultDesc,        baiqishi.strategy_set AS baiqishi_strategySet,        baiqishi.flow_no AS baiqishi_flowNo,        baiqishi.gmt_datetime AS baiqishi_gmtDatetime,        baiqishi.upt_datetime AS baiqishi_uptDatetime    </sql>    <sql id="mBaiqishiSql">        id AS id,        user_id AS userId,        result_code AS resultCode,        result_desc AS resultDesc,        strategy_set AS strategySet,        flow_no AS flowNo,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="selectByCondition" parameterType="Baiqishi" resultMap="rmBaiqishi">    select    <include refid="mBaiqishiSql"></include>    from baiqishi    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="userId!= null">            AND user_id=#{userId}        </if>        <if test="resultCode!= null">            AND result_code=#{resultCode}        </if>        <if test="resultDesc!= null">            AND result_desc=#{resultDesc}        </if>        <if test="strategySet!= null">            AND strategy_set=#{strategySet}        </if>        <if test="flowNo!= null">            AND flow_no=#{flowNo}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>    </where>    </select>    </mapper>