<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.UserMessageMapper">    <resultMap type="com.rongke.model.UserMessage" id="rmUserMessage">        <id property="id" column="id" />        <result property="userId" column="userId"/>        <result property="type" column="type"/>        <result property="status" column="status"/>        <result property="content" column="content"/>        <result property="title" column="title"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>        <association property="user" javaType="com.rongke.model.User" >            <id property="id" column="user_id" />        </association>    </resultMap>    <resultMap type="com.rongke.model.UserMessage" id="mUserMessage">        <id property="id" column="userMessage_id" />        <result property="userId" column="userMessage_userId"/>        <result property="type" column="userMessage_type"/>        <result property="status" column="userMessage_status"/>        <result property="content" column="userMessage_content"/>        <result property="title" column="userMessage_title"/>        <result property="gmtDatetime" column="userMessage_gmtDatetime"/>        <result property="uptDatetime" column="userMessage_uptDatetime"/>        <association property="user" javaType="com.rongke.model.User" >            <id property="id" column="user_id" />        </association>    </resultMap>    <sql id="mUserMessagePkSql">        user_message.id AS userMessage_id,        user_message.user_id AS userMessage_userId,        user_message.type AS userMessage_type,        user_message.status AS userMessage_status,        user_message.content AS userMessage_content,        user_message.title AS userMessage_title,        user_message.gmt_datetime AS userMessage_gmtDatetime,        user_message.upt_datetime AS userMessage_uptDatetime    </sql>    <sql id="mUserMessageSql">        id AS id,        user_id AS userId,        type AS type,        status AS status,        content AS content,        title AS title,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="selectByCondition" parameterType="UserMessage" resultMap="rmUserMessage">    select    <include refid="mUserMessageSql"></include>    from user_message    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="userId!= null">            AND user_id=#{userId}        </if>        <if test="type!= null">            AND type=#{type}        </if>        <if test="status!= null">            AND status=#{status}        </if>        <if test="content!= null">            AND content=#{content}        </if>        <if test="title!= null">            AND title=#{title}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>    </where>    </select>    </mapper>