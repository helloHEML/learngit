<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.UserContactMapper">    <resultMap type="com.rongke.model.UserContact" id="rmUserContact">        <id property="id" column="id" />        <result property="userId" column="userId"/>        <result property="name" column="name"/>        <result property="phone" column="phone"/>        <result property="relation" column="relation"/>        <result property="emergencyContact" column="emergencyContact"/>        <result property="gmtDatetime" column="gmtDatetime"/>        <result property="uptDatetime" column="uptDatetime"/>        <association property="user" javaType="com.rongke.model.User" >            <id property="id" column="user_id" />        </association>    </resultMap>    <resultMap type="com.rongke.model.UserContact" id="mUserContact">        <id property="id" column="userContact_id" />        <result property="userId" column="userContact_userId"/>        <result property="name" column="userContact_name"/>        <result property="phone" column="userContact_phone"/>        <result property="relation" column="userContact_relation"/>        <result property="emergencyContact" column="userContact_emergencyContact"/>        <result property="gmtDatetime" column="userContact_gmtDatetime"/>        <result property="uptDatetime" column="userContact_uptDatetime"/>        <association property="user" javaType="com.rongke.model.User" >            <id property="id" column="user_id" />        </association>    </resultMap>    <sql id="mUserContactPkSql">        user_contact.id AS userContact_id,        user_contact.user_id AS userContact_userId,        user_contact.name AS userContact_name,        user_contact.phone AS userContact_phone,        user_contact.relation AS userContact_relation,        user_contact.emergency_contact AS userContact_emergencyContact,        user_contact.gmt_datetime AS userContact_gmtDatetime,        user_contact.upt_datetime AS userContact_uptDatetime    </sql>    <sql id="mUserContactSql">        id AS id,        user_id AS userId,        name AS name,        phone AS phone,        relation AS relation,        emergency_contact AS emergencyContact,        gmt_datetime AS gmtDatetime,        upt_datetime AS uptDatetime    </sql>    <select id="selectByCondition" parameterType="UserContact" resultMap="rmUserContact">    select    <include refid="mUserContactSql"></include>    from user_contact    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="userId!= null">            AND user_id=#{userId}        </if>        <if test="name!= null">            AND name=#{name}        </if>        <if test="phone!= null">            AND phone=#{phone}        </if>        <if test="relation!= null">            AND relation=#{relation}        </if>        <if test="emergencyContact!= null">            AND emergency_contact=#{emergencyContact}        </if>        <if test="gmtDatetime!= null">            AND gmt_datetime=#{gmtDatetime}        </if>        <if test="uptDatetime!= null">            AND upt_datetime=#{uptDatetime}        </if>    </where>    </select>    </mapper>