<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.AdminRolePermissionMapper">    <resultMap type="com.rongke.model.AdminRolePermission" id="rmAdminRolePermission">        <id property="id" column="id" />        <result property="adminRoleId" column="adminRoleId"/>        <result property="adminPermissionId" column="adminPermissionId"/>    </resultMap>    <resultMap type="com.rongke.model.AdminRolePermission" id="mAdminRolePermission">        <id property="id" column="adminRolePermission_id" />        <result property="adminRoleId" column="adminRolePermission_adminRoleId"/>        <result property="adminPermissionId" column="adminRolePermission_adminPermissionId"/>    </resultMap>    <sql id="mAdminRolePermissionPkSql">        admin_role_permission.id AS adminRolePermission_id,        admin_role_permission.admin_role_id AS adminRolePermission_adminRoleId,        admin_role_permission.admin_permission_id AS adminRolePermission_adminPermissionId    </sql>    <sql id="mAdminRolePermissionSql">        id AS id,        admin_role_id AS adminRoleId,        admin_permission_id AS adminPermissionId    </sql>    <select id="selectByCondition" parameterType="AdminRolePermission" resultMap="rmAdminRolePermission">    select    <include refid="mAdminRolePermissionSql"></include>    from admin_role_permission    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="adminRoleId!= null">            AND admin_role_id=#{adminRoleId}        </if>        <if test="adminPermissionId!= null">            AND admin_permission_id=#{adminPermissionId}        </if>    </where>    </select>    </mapper>