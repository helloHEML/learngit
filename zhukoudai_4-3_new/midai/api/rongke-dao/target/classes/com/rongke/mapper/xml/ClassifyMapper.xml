<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.ClassifyMapper">    <resultMap type="com.rongke.model.Classify" id="rmClassify">        <id property="id" column="id" />        <result property="name" column="name"/>        <result property="initial_price" column="initialPrice"/>        <result property="picUrl" column="picUrl"/>    </resultMap>    <resultMap type="com.rongke.model.Classify" id="mClassify">        <id property="id" column="classify_id" />        <result property="name" column="classify_name"/>        <result property="initial_price" column="classify_initialPrice"/>        <result property="picUrl" column="classify_picUrl"/>    </resultMap>    <sql id="mClassifyPkSql">        classify.id AS classify_id,        classify.name AS classify_name,        classify.initial_price AS classify_initialPrice,        classify.pic_url AS classify_picUrl    </sql>    <sql id="mClassifySql">        id AS id,        name AS name,        initial_price AS initialPrice,        pic_url AS picUrl    </sql>    <select id="selectByCondition" parameterType="Classify" resultMap="rmClassify">    select    <include refid="mClassifySql"></include>    from classify    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="name!= null">            AND name=#{name}        </if>        <if test="picUrl!= null">            AND pic_url=#{picUrl}        </if>    </where>    </select>    </mapper>