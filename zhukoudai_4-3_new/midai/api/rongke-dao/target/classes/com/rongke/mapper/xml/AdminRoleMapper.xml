<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.rongke.mapper.AdminRoleMapper">    <resultMap type="com.rongke.model.AdminRole" id="rmAdminRole">        <id property="id" column="id" />        <result property="name" column="name"/>        <result property="available" column="available"/>    </resultMap>    <resultMap type="com.rongke.model.AdminRole" id="mAdminRole">        <id property="id" column="adminRole_id" />        <result property="name" column="adminRole_name"/>        <result property="type" column="adminRole_type"/>        <result property="available" column="adminRole_available"/>    </resultMap>    <sql id="mAdminRolePkSql">        admin_role.id AS adminRole_id,        admin_role.name AS adminRole_name,        admin_role.type AS adminRole_type,        admin_role.available AS adminRole_available    </sql>    <sql id="mAdminRoleSql">        id AS id,        name AS name,        available AS available    </sql>    <select id="selectByCondition" parameterType="AdminRole" resultMap="rmAdminRole">    select    <include refid="mAdminRoleSql"></include>    from admin_role    <where>        <if test="id!= null">            AND id=#{id}        </if>        <if test="name!= null">            AND name=#{name}        </if>        <if test="available!= null">            AND available=#{available}        </if>    </where>    </select>    </mapper>