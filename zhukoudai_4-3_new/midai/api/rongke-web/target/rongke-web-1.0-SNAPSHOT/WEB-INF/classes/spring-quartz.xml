<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd ">    <!--开始定时任务-->    <bean id="cardpayjob" class="com.rongke.web.quartz.CardpayTimter"></bean>    <bean id="orderjob" class="com.rongke.web.quartz.ReportStatistics"></bean>    <!--统计订单开始计算-->    <bean id="SumJobDetail" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">        <property name="targetObject" ref="orderjob"/>        <property name="targetMethod" value="insertOrderSum"/>        <property name="concurrent" value="false"/>    </bean>    <!--订单扣款-->    <bean id="dayJobDetail" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">        <property name="targetObject" ref="cardpayjob" />        <property name="targetMethod" value="authbindcardrequest"/>        <property name="concurrent" value="false"/>    </bean>    <!--短信催款-->    <bean id="dayJobsms" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">        <property name="targetObject" ref="cardpayjob" />        <property name="targetMethod" value="sendnotify"/>        <property name="concurrent" value="false"/>    </bean>    <!-- 认证审核员 分配-->    <bean id="assignmentTask" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">        <property name="targetObject" ref="cardpayjob" />        <property name="targetMethod" value="assignmentTask"/>        <property name="concurrent" value="false"/>    </bean>    <!--订单分配-->    <bean id="orderTask" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">        <property name="targetObject" ref="cardpayjob" />        <property name="targetMethod" value="orderTask"/>        <property name="concurrent" value="false"/>    </bean>    <!--审核单分配-->    <bean id="evaTask" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">        <property name="targetObject" ref="cardpayjob" />        <property name="targetMethod" value="evaluationTask"/><!--方法-->        <property name="concurrent" value="false"/><!-- 设置false 当有未执行的 定时器的时候 -->    </bean>    <!--系统定时审核-->    <bean id="MachineAudit" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">        <property name="targetObject" ref="cardpayjob" />        <property name="targetMethod" value="AutomaticAudit"/><!--方法-->        <property name="concurrent" value="false"/><!-- 设置false 当有未执行的 定时器的时候 -->    </bean>    <!--打款分配-->    <!--        <bean id="evaluationTask" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">            <property name="targetObject" ref="cardpayjob" />         &lt;!&ndash;   <property name="targetMethod" value="evaluationTask"/>&ndash;&gt;            <property name="concurrent" value="false"/>        </bean>    -->    <!--生成订单统计表-->    <bean id="insertOrderTask" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">        <property name="jobDetail" ref="SumJobDetail"/>        <!--每天早上一点执行一次 -->        <property name="cronExpression" value=" 0 0 1 * * ?"/>    </bean>    <!--每天定时触发 代扣 -->    <bean id="dayTrigger"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">        <property name="jobDetail" ref="dayJobDetail" />        <property name="cronExpression" value="0 20 11 * * ? " /><!--  0 20 11 * * ?  -->    </bean>                                                    <!-- 0 0/10 11-18 * * ?  -->    <!--每天定时触发 短信通知-->    <bean id="dayTrigger2"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">        <property name="jobDetail" ref="dayJobsms" />        <property name="cronExpression" value="0 0 12,18 * * ? " /> <!-- 0 0 5 * * ?  --><!--  0 0/10 11-18 * * ?  -->    </bean>    <!--每天定时触发 审核任务分配-->    <bean id="dayTriggerAssignmentTask"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">        <property name="jobDetail" ref="assignmentTask" />        <property name="cronExpression" value="0 0 6 * * ?"/> <!-- 0 0/5 14-18 * * ?  --><!-- 0 0/10 * * * ? -->    </bean>    <!--每天定时触发 订单分配-->    <bean id="dayTriggerOrderTask"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">        <property name="jobDetail" ref="orderTask" />        <property name="cronExpression" value="0 0 7 * * ?" /><!-- 0 0/5 14-18 * * ? -->    </bean>    <!--每天定时触发 审核单分配-->    <bean id="dayTriggerEvaTask"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">        <property name="jobDetail" ref="evaTask" />        <property name="cronExpression" value="0 0/5 * * * ?" />        <!-- 0 0/5 14-18 * * ? --><!--每天下午2-6点 整点开始每5分钟执行一次-->        <!-- 0 0/5 14-18 * * ? --><!--每天下午2-6点 每5分钟执行一次-->    </bean>    <bean id="dayTriggerMachineAudit"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">        <property name="jobDetail" ref="MachineAudit" />        <!--<property name="cronExpression" value="*/5 * * * * ?" />-->        <property name="cronExpression" value="0 0/8 * * * ?" />        <!-- 0 0/5 14-18 * * ? --><!--每天下午2-6点 整点开始每5分钟执行一次-->        <!-- 0 0/5 14-18 * * ? --><!--每天下午2-6点 每5分钟执行一次-->    </bean>    <!--每天定时触发 打款分配-->    <!--<bean id="dayTriggerEvaluationTask"  class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">            <property name="jobDetail" ref="evaluationTask" />            <property name="cronExpression" value="0 0/10 * * * ?" />        </bean>-->    <!-- 触发器工厂，将所有的定时任务都注入工厂-->    <bean autowire="no" class="org.springframework.scheduling.quartz.SchedulerFactoryBean">        <property name="jobDetails">            <list>                <!--审核分配-->                <!--<ref bean="evaTask"/>-->                <!--定时扣款-->                <!--<ref bean="dayJobDetail"/>-->                <!--短信催款-->                <ref bean="dayJobsms"/>                <!--审核任务分配-->                <!--<ref bean="assignmentTask"/>-->                <!--订单分配-->                <!--<ref bean="orderTask"/>-->                <!--<ref bean="evaluationTask"/>-->                <!--<ref bean="MachineAudit"/>-->            </list>        </property>        <property name="triggers">            <list>                <!--审核分配-->                <!--<ref bean="dayTriggerEvaTask"/>-->                <!--定时扣款-->                <!--<ref bean="dayTrigger"/>-->                <!--短信催款-->                <!--<ref bean="dayTrigger2"/>-->                <!--审核任务分配-->                <!--<ref bean="dayTriggerAssignmentTask"/>-->                <!--订单分配-->                <!--<ref bean="dayTriggerOrderTask"/>-->                <!--<ref bean="dayTriggerEvaluationTask"/>-->                <!--自动审核-->                <!--<ref bean="dayTriggerMachineAudit"/>-->                <!--<ref bean="insertOrderTask"/>-->            </list>        </property>    </bean>    <!--定时任务结束--></beans>